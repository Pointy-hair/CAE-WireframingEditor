<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs
    title="CAE-WireframingEditor"
    description="Wireframing Editor for the CAE"
    author="Mario Rosenstengel"
    author_email="rosenst@dbis.rwth-aachen.de"
    width="650"
    height="650">
  <!--recalculate max-height and width-heigth if the widget size in widget.xml is changes-->

	
  </ModulePrefs>
  <Content type="html">
    <![CDATA[
         
  <link href="https://rwth-acis.github.io/CAE-WireframingEditor/role/css/style.css" rel="stylesheet">


<script src="manifest.js"></script>

<script src="style.js"></script>

<script src="vendor.js"></script>

<script src="mxVendor.js"></script>

<script src="jVendor.js"></script>

<script src="yVendor.js"></script>

<script src="app.js"></script>


  <div id="paletteContainer">
    <div id="palette"></div>
    <div class="hsplit"></div>
  </div>
  <div id="mainContainer">
    <div id='toolbox'>
  </div>
  <!--recalculate max-height and width-heigth if the widget size in widget.xml is changes-->
    <div id="wireframeContainer" style="max-height: 615px; max-width: 555px; overflow:auto" >
      <div id="wireframeWrap">
        <div id="draggingBar">
          <span class="ui-icon ui-icon-closethick"></span>
          <span class="ui-icon ui-icon-minusthick"></span>
          <span class="ui-icon ui-icon-plusthick"></span>
        </div>
        <div id="wireframe"></div>
      </div>
    </div>
  </div>
  </div>
  <div id="userList">
    <table style="background-color: white"></table>
  </div>
  <div id="init" title="Initialization">
    <div id="loading">
      <div id="progressbar"><div class="progress-label">0%</div></div>
      <p> Synchronization with Yjs </p>
      <p> Initiliaze Wireframe model </p>
      <p> Initiliaze Live Mapper </p>
      <p> Login to ROLE space </p>
    </div>
  </div>
    ]]>
  </Content>
</Module>
